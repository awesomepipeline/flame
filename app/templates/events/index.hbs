<div class="container">
  <div class="row">
    <div class="col m8 offset-m2">
        <div id="hosting">
          {{#each events as |event|}}
            <div>{{hosting-card-summary card-data=event deleteEvent="delete"}}</div>

          {{else}} 
          
          <div class="card grey lighten-5">
            <div class="card-content grey-text text-darken-2">
              You have not created any events. Would you like to {{#link-to 'events.new'}}create one now{{/link-to}}?
            </div>
          </div>          

          {{/each}}
       </div>
    </div>
  </div>
</div>
